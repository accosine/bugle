{
  "name": "prismatic",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "tsc && mkdir -p dist && cp src/index.d.ts dist/index.d.ts && npx prisma generate",
    "prisma:bootstrap": "prisma generate && prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:introspect": "prisma introspect",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:seed": "prisma db seed",
    "prisma:setup": "npx prisma generate && npx prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "test": "echo \"Warning: no prismatic tests specified\" && exit 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "4.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/js-yaml": "^4.0.5",
    "bcryptjs": "^2.4.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "prisma": "4.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.4"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
