{
  "name": "letterpress",
  "version": "0.0.0",
  "description": "",
  "types": "dist/types.d.ts",
  "scripts": {
    "parcel:watch": "parcel watch",
    "parcel:build": "parcel build",
    "parcel:which": "which parcel && sleep 60"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "parcel": "2.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^4.8.3"
  },
  "targets": {
    "cjs": {
      "source": "components/index.ts",
      "isLibrary": true,
      "includeNodeModules": false,
      "context": "node",
      "outputFormat": "commonjs"
    },
    "esm": {
      "source": "components/index.ts",
      "isLibrary": true,
      "includeNodeModules": false,
      "context": "node",
      "outputFormat": "esmodule"
    },
    "styles": {
      "source": "styles/test.scss"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/index.css": {
      "import": "./dist/styles/test.css",
      "require": "./dist/styles/test.css"
    }
  }
}
